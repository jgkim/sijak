  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2 || ^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "husky": "^0.13.2",
    "rimraf": "^2.6.1"
  },
  "engines": {
    "node": "^6.10.0",
    "yarn": "^0.21.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jgkim/sijak"
  },
