{
  "name": "Sijak",
  "version": "0.0.3",
  "description": "A Stepping Stone for Full-stack Javascript Development",
  "author": "James G. Kim <jgkim@jayg.org> (http://jayg.org/)",
  "main": "index.js",
  "scripts": {
    "start": "babel-node src",
    "start:production": "node build",
